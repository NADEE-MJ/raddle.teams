# Admin password for authentication
ADMIN_PASSWORD=your_admin_password_here

# Database URL
# SQLite:
DATABASE_URL=sqlite:///databases/raddle.db
# PostgreSQL:
# DATABASE_URL=postgresql://user:password@localhost:5432/raddle_teams

# Server configuration (optional, can be overridden by CLI flags)
RADDLE_HOST=127.0.0.1
RADDLE_PORT=9001

# Logging level (trace, debug, info, warn, error)
RUST_LOG=info

# Environment (set to "testing" for test mode)
# RADDLE_ENV=testing
