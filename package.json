{
    "name": "raddle-teams-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build": "vite build",
        "build:watch": "vite build --watch",
        "preserver": "npm run build",
        "server": "poetry run python bin/run_server.py development",
        "pretesting-server": "npm run build",
        "testing-server": "poetry run python bin/run_server.py testing",
        "lint": "eslint . --report-unused-disable-directives --max-warnings 15",
        "lint:fix": "eslint . --fix",
        "lint:python": "poetry run ruff check .",
        "pretest": "npm run build",
        "test": "poetry run python bin/run_tests.py",
        "trace": "playwright show-trace",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "format:python": "poetry run ruff format .",
        "type-check": "tsc --noEmit",
        "check": "npm run type-check && npm run lint && npm run format:check"
    },
    "dependencies": {
        "prettier-plugin-tailwindcss": "^0.6.14",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router-dom": "^7.8.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@tailwindcss/postcss": "^4.1.12",
        "@tailwindcss/vite": "^4.1.12",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.8",
        "@typescript-eslint/eslint-plugin": "^8.41.0",
        "@typescript-eslint/parser": "^8.41.0",
        "@vitejs/plugin-react": "^5.0.2",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.34.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "playwright": "^1.55.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.1.12",
        "typescript": "^5.9.2",
        "vite": "^7.1.3"
    }
}
